      <version>1.0</version>
    </dependency>
    <dependency>
      <groupId>com.bea.xml</groupId>
      <artifactId>jsr173-api</artifactId>
      <version>1.0</version>
    </dependency>
    <dependency>
      <groupId>net.java.dev.stax-utils</groupId>
      <artifactId>stax-utils</artifactId>
      <version>20060502</version>
    </dependency>
    <dependency>
